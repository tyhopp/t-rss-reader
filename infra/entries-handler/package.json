{
  "private": "true",
  "name": "entries-handler",
  "description": "Lambda function that fetches an RSS feed and returns its entries as JSON",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "del dist entries-handler.zip",
    "build": "ncc build ./src/index.ts --out dist",
    "zip": "zip -r entries-handler.zip ./dist",
    "test": "vitest"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.0",
    "linkedom": "^0.14.22"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/jsonwebtoken": "^9.0.1",
    "@vercel/ncc": "^0.36.1",
    "del-cli": "^5.0.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  }
}
